<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomNavigationStyle" parent="Widget.Design.BottomNavigationView">
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/bottom_icon_txt_color</item>
        <item name="boxStrokeWidth">2dp</item>

    </style>
    <style name="ToolbarTitleStyle" parent="TextAppearance.MaterialComponents.Headline6">
        <item name="android:textSize">16sp</item>
        <item name="fontFamily">@font/gilory_bold</item>
        <item name="textColor">@color/textColor</item>
    </style>
    <style name="CustomDatePickerDialog" parent="ThemeOverlay.MaterialComponents.Dialog">
        <item name="colorPrimary">@color/btn_color</item> // Asosiy brend rangi
        <item name="android:textColor">@color/textColor</item>
        <item name="colorSecondary">@color/btn_color</item>
        <item name="selectedDotColor">@color/btn_color</item>
        <item name="android:datePickerMode">spinner</item>
        <item name="android:selectedWeekBackgroundColor">@color/btn_color</item>
        <item name="android:selectedDateVerticalBar">@color/btn_color</item>
    </style>

</resources>